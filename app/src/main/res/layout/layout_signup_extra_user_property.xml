<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/color4"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <EditText
        android:id="@+id/age"
        style="@style/com_kakao_profile_property_value"
        android:hint="태어난 해"
        android:text="25"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp" />

    <EditText
        android:id="@+id/gender"
        style="@style/com_kakao_profile_property_value"
        android:hint="성별"
        android:text="남자"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"/>

    <Button
        android:id="@+id/btn_signup"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="@dimen/com_kakao_profile_property_margin"
        android:layout_marginLeft="@dimen/com_kakao_profile_property_margin"
        android:layout_marginRight="@dimen/com_kakao_profile_property_margin"
        android:textSize="@dimen/com_kakao_profile_property_text"
        android:textStyle="bold"
        style="@style/button"
        android:background="@drawable/selector_button_01"
        android:paddingLeft="@dimen/com_kakao_padding"
        android:paddingRight="@dimen/com_kakao_padding"
        android:text="@string/signup_button"/>


</LinearLayout>